<AnchorPane prefHeight="676.0" prefWidth="600.0" styleClass="middleocean-background" stylesheets="@style.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.lez5.ControllerAddRequestQuesturaScene">
   <children>
